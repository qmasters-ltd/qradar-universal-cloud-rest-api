<?xml version="1.0" encoding="UTF-8" ?>
<WorkflowParameterValues xmlns="http://qradar.ibm.com/UniversalCloudRESTAPI/WorkflowParameterValues/V2">

    <!-- "orgID" - Atlassian Organization ID. -->
    <Value name="orgID" value=""/>
    <!-- "apiToken" - Atlassian API token for QRadar (required) -->
    <Value name="apiToken" value=""/>

    <!-- "identifier" - The log source identifier. Default: Atlassian-AuditLogs (optional) -->
    <Value name="identifier" value="Atlassian-AuditLogs"/>

    <!-- "timeZone" - Select your time zone (Default=UTC). (optional) -->
    <Value name="timeZone" value="UTC"/>
    <!-- "startTime" - Number of days to look back for initial event ingestion. Useful for historical data collection on first run. Default: 7 days (optional) -->
    <Value name="startTime" value="7"/>
    <!-- "eventsPerFetch" - Maximum number of event records to fetch in each API call. Large values may increase performance but can cause timeout errors, (max 500). Default: 100  (optional) -->
    <Value name="eventsPerFetch" value="100"/>

</WorkflowParameterValues>